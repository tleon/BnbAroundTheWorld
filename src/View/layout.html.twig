  {% block javascript %}{% endblock %}
  <script>
    function scrollFunction() {
      (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) ? $("#modalBtn").css('display', 'block') : $("#modalBtn").css('display', 'none');
    }
    $('#modalBtn').click(function () {
      $('body,html').animate({
        scrollTop: 0
      }, 800);
      return false;
    });
    $(document).ready(function () {
      $('.sidenav').sidenav({ edge: 'right' });
      $('.modal').modal({ opacity: 0 });
      $('.parallax').parallax();
      $('select').formSelect();
      $('.datepicker').datepicker();
      $('#fixed-action-btn').floatingActionButton();
      $('.myDropdown-trigger').dropdown();
      window.onscroll = function () {
        scrollFunction()
      };
    });
  </script>
