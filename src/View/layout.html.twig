  <div class="modal bg-lg-white" id="modal1">
    <div class="modal-content">
      <h4 class="color-dark-blue">Connexion</h4>
      <div class="row">
        <form action="/Home/index" class="col s12" method="POST">
          <div class="row">
            <div class="input-field col s12">

              <input class="validate" id="username" name="username" type="text">
              <label for="username">Nom d'utilisateur</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">

              <input class="validate" id="password" name="password" type="password">
              <label for="password">Mot de passe</label>
            </div>
          </div>

          <div class="right-align">

            <a class="color-dark-blue" href="/Home/SignIn">Inscrivez vous !
            </a>
            <input class="modal-close waves-effect waves-green btn-flat" type="submit" value="Connexion">
          </div>
        </form>
      </div>
    </div>
  </div>
